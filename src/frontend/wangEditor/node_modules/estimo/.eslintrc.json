{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "browser": false,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "no-bitwise": 0,
    "semi": 0,
    "no-return-await": 0,
    "no-await-in-loop": 0,
    "no-restricted-syntax": 0,
    "import/prefer-default-export": 0,
    "semi-style": 0
  }
}
